{
  "expo": {
    "name": "escola-segura",
    "slug": "escola-segura",
    "version": "1.0.0",
    "scheme": "com.capelaniabrasil.escolasegura",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir Escola Segura ver sua localização."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.capelaniabrasil.escolasegura",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f215db1e-8c41-4081-9dd4-182744df253f"
      }
    },
    "owner": "lonwaychao"
  }
}
